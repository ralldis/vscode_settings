      "INCOMPLETE:": {
        "icon": "alert",
        "foreground": "white",
        "background": "mediumvioletred",
        "iconColour": "mediumvioletred"
      },
      "BUG:": {
        "icon": "bug",
        "foreground": "white",
        "background": "crimson",
        "iconColour": "crimson"
      },
      "SEE NOTES": {
        "icon": "note",
        "foreground": "white",
        "background": "teal",
        "iconColour": "teal"
      },
      "POST": {
        "icon": "share",
        "foreground": "white",
        "background": "green",
        "iconColour": "green"
      },
      "[ ]:": {
        "icon": "check",
        "foreground": "black",
        "background": "#ff000080",
        "iconColour": "#ff000080",
        "type": "tag"
      },
      "[x]:": {
        "icon": "check",
        "foreground": "white",
        "background": "green",
        "iconColour": "green",
        "type": "tag"
      }
    },
    "todo-tree.general.tags": [
      "BUG:",
      "SEE NOTES",
      "HACK:",
      "FIXME:",
      "RECHECK:",
      "INCOMPLETE:",
      "TODO:",
      "NOTE:",
      "POST",
      "USEFUL:",
      "LEARN:",
      "COMMENT:",
      "[ ]:",
      "[x]:"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS).*(\\n\\s*//\\s{2,}.*)*",

